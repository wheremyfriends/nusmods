{
  "name": "react-kawaii",
  "version": "0.18.0",
  "description": "React Kawaii is a library of cute SVG illustrations (react components). Ideal if you want to give some cuteness and personality to your react application.",
  "main": "lib/index.js",
  "module": "lib/es/index.js",
  "scripts": {
    "test": "",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "lib:build": "webpack -p --progress && babel src --out-dir lib/es/ && babel native --out-dir lib/native/",
    "precommit": "pretty-quick --staged",
    "prepare": "npm run lib:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miukimiu/react-kawaii.git"
  },
  "keywords": [
    "library",
    "react",
    "component",
    "svg",
    "illustration",
    "kawaii",
    "icon",
    "ui",
    "animation",
    "icons",
    "cute",
    "mbe",
    "cartoons",
    "design"
  ],
  "author": "Miukimiu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miukimiu/react-kawaii/issues"
  },
  "homepage": "https://github.com/miukimiu/react-kawaii#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.2",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "popmotion": "^8.5.3",
    "postcss": "^7.0.6",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flexbox-grid": "^2.1.2",
    "react-ga": "^2.5.3",
    "react-icons": "^4.4.0",
    "react-plx": "^1.3.7",
    "react-style-proptype": "^3.2.1",
    "react-styleguidist": "^8.0.6",
    "remcalc": "^1.0.10",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "react": "15.3.0 - 18.x"
  },
  "engines": {
    "node": ">=7.4.0",
    "npm": ">=4.0.5"
  },
  "sideEffects": false
}
